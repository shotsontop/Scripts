local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BTEP [SHOTSWARE]", "DarkTheme")

local InfCashTab = Window:NewTab("Infinite Cash")
local InfCashSection = InfCashTab:NewSection("Infinite Cash")

local function infCashScript()
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GiveQuestCash"):FireServer(math.huge, true)
end

local InfCashButton = InfCashSection:NewButton("Inf Cash", "Gives infinite cash", infCashScript)

local MiscellaneousTab = Window:NewTab("Miscellaneous")
local MiscellaneousSection = MiscellaneousTab:NewSection("Infinite Yield")

local InfYieldButton = MiscellaneousSection:NewButton("Execute Infinite Yield", "Runs Infinite Yield", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

local ToggleUITab = Window:NewTab("Toggle UI")
local ToggleUISection = ToggleUITab:NewSection("Toggle UI")

local function toggleUI()
    Library:ToggleUI()
end

local UIS = game:GetService("UserInputService")
_G.KeybindUI = Enum.KeyCode.V

local function changeKeybind(txt)
    local key = string.upper(string.sub(txt, 1, 1))
    if Enum.KeyCode[key] then
        _G.KeybindUI = Enum.KeyCode[key]
    else
        warn("Invalid key.")
    end
end

ToggleUISection:NewTextBox("Toggle UI Keybind", "Enter a key to toggle UI", changeKeybind)

UIS.InputBegan:Connect(function(input, k)
    if k then return end
    if input.KeyCode == _G.KeybindUI then
       toggleUI()
    end
end)

local CreditsTab = Window:NewTab("Credits")
local CreditsSection = CreditsTab:NewSection("Script Maker: Shots")
local CreditsSection2 = CreditsTab:NewSection("My Discord: kmlt")
local CreditsSection3 = CreditsTab:NewSection("My Biolinks: https://guns.lol/sh0ts")
local CreditsSection4 = CreditsTab:NewSection("My Biolinks: https://feds.lol/shots")
